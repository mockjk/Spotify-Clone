{
  "expo": {
    "name": "Spotify-Clone",
    "slug": "Spotify-Clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/Spotify-app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/Spotify-app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#191414"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/Spotify-app-icon.png",
        "backgroundColor": "#191414"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
